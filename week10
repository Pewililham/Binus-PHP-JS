// Tampilkan menu konversi
console.log("MENU KONVERSI SUHU");
console.log("1. Celcius ke Fahrenheit");
console.log("2. Celcius ke Kelvin");
console.log("3. Fahrenheit ke Celcius");
console.log("4. Fahrenheit ke Kelvin");
console.log("5. Kelvin ke Celcius");
console.log("6. Kelvin ke Fahrenheit");

// Input menu
let menu = parseInt(prompt("Masukkan nomor menu yang ingin dikonversi (1-6):"));

let suhu;
let hasil;

// Proses konversi
if (menu == 1) {
    suhu = parseInt(prompt("Masukkan suhu Celcius:"));
    hasil = (suhu * 9 / 5) + 32;
    console.log("Hasil = " + hasil + " Fahrenheit");

} else if (menu == 2) {
    suhu = parseInt(prompt("Masukkan suhu Celcius:"));
    hasil = suhu + 273;
    console.log("Hasil = " + hasil + " Kelvin");

} else if (menu == 3) {
    suhu = parseInt(prompt("Masukkan suhu Fahrenheit:"));
    hasil = (suhu - 32) * 5 / 9;
    console.log("Hasil = " + hasil + " Celcius");

} else if (menu == 4) {
    suhu = parseInt(prompt("Masukkan suhu Fahrenheit:"));
    hasil = (suhu - 32) * 5 / 9 + 273;
    console.log("Hasil = " + hasil + " Kelvin");

