/*
// Input sisi segitiga
let A = parseInt(prompt("Masukkan sisi A:"));
let B = parseInt(prompt("Masukkan sisi B:"));
let C = parseInt(prompt("Masukkan sisi C:"));

// Cek bukan segitiga
if (A <= 0 || B <= 0 || C <= 0) {
    console.log("Bukan Segitiga");

} else if (A + B <= C || A + C <= B || B + C <= A) {
    console.log("Bukan Segitiga");

}
// Segitiga sama sisi
else if (A == B && B == C) {
    console.log("Segitiga Sama Sisi");

}
// Segitiga siku-siku (bonus)
else if (
    A * A == B * B + C * C ||
    B * B == A * A + C * C ||
    C * C == A * A + B * B
) {
    console.log("Segitiga Siku-siku");

}
// Segitiga sama kaki
else if (A == B || A == C || B == C) {
    console.log("Segitiga Sama Kaki");

}
// Segitiga sembarang
else {
    console.log("Segitiga Sembarang");
}
*/

// Input nilai A, B, dan C
let A = parseInt(prompt("Masukkan nilai A: "));
let B = parseInt(prompt("Masukkan nilai B: "));
let C = parseInt(prompt("Masukkan nilai C: "));

let D;

// Proses pengecekan
if (A == 0) {
    console.log("Bukan Persamaan Kuadrat");

} else {
    // Hitung diskriminan
    D = (B * B) - (4 * A * C);

    // Filtering jenis akar
    if (D > 0) {
        console.log("Merupakan Akar Berbeda");
        console.log("Nilai Diskriminan = " + D);

    } else if (D == 0) {
        console.log("Merupakan Akar Kembar");
        console.log("Nilai Diskriminan = " + D);

    } else {
        console.log("Merupakan Akar Imajiner");
        console.log("Nilai Diskriminan = " + D);
    }
}




